{
  "compilerOptions": {
    "lib": [
      "es2020"
    ],
    "target": "ES2020",
    "module": "commonjs",
    "removeComments": true,
    "allowJs": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "typeRoots": ["node_modules/@types"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
